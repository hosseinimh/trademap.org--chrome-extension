{
  "name": "Trademap",
  "version": "1.0.0",
  "description": "Download HS codes. Developed by: hosseinimh@gmail.com. Release date: 04/15/2023",
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "downloads",
    "clipboardRead",
    "clipboardWrite"
  ],
  "host_permissions": ["*://*/*"],
  "background": {
    "type": "module",
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["js/utils.js", "bs/bootstrap.min.css"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/icon-16x16.png",
      "24": "assets/icon.png",
      "32": "assets/icon-32x32.png",
      "48": "assets/icon.png",
      "128": "assets/icon-128x128.png",
      "192": "assets/icon-192x192.png"
    },
    "default_title": "Trademap",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icon-16x16.png",
    "24": "assets/icon.png",
    "32": "assets/icon-32x32.png",
    "48": "assets/icon.png",
    "128": "assets/icon-128x128.png",
    "192": "assets/icon-192x192.png"
  },
  "manifest_version": 3
}
